<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_stock_move_history" model="ir.ui.view">
        <field name="name">Stock Move Report at Date</field>
        <field name="model">stock.move.consolidation</field>
        <field name="arch" type="xml">
            <form string="Choose your date">
                <group>
                    <group>
                        <field name="date_from"/>
                    </group>
                    <group>
                        <field name="date_to"/>
                    </group>
                </group>

                <footer>
                    <button name="stock_move_till_date" string="View" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>
